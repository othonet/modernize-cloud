{
  "name": "private-cloud",
  "version": "1.0.0",
  "description": "Modernize Cloud - Cloud privada com sincronização ponto a ponto",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "dev:all": "concurrently \"npm run build:css\" \"npm run dev\" --names \"CSS,SERVER\" --prefix-colors \"blue,green\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./public/css/style.css --watch",
    "generate:icons": "node generate-icons.js",
    "pwa:setup": "npm run generate:icons",
    "cap:sync": "node scripts/capacitor-index.js create && npx cap sync && node scripts/capacitor-index.js remove",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "cap:build": "npm run build:css && npm run cap:sync"
  },
  "keywords": [
    "cloud",
    "storage",
    "sync",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@capacitor/app": "^7.1.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/network": "^7.0.2",
    "@capacitor/share": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@prisma/client": "^5.7.1",
    "adm-zip": "^0.5.16",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "concurrently": "^9.2.1",
    "prisma": "^5.7.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.3.6"
  }
}
